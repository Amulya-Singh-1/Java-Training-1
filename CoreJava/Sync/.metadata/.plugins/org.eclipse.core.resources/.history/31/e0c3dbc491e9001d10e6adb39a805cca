package CollectionsPractise;

import java.util.Vector;

public class Vectoreg {
	public static void main(String[] args) {
		Vector<Integer> vec =new Vector<>(4, 2);
		vec.add(1);
		vec.add(2);
		vec.add(3);
		vec.add(4);
		System.out.println(vec.size());
		System.out.println(vec.capacity());
		vec.add(5);
		System.out.println(vec.size());
		System.out.println(vec.capacity());
		vec.add(6);
		System.out.println(vec.size());
		System.out.println(vec.capacity());
		vec.add(7);
		System.out.println(vec.size());
		System.out.println(vec.capacity());
	}

}
